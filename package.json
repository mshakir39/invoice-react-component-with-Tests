  "name": "cupola",
  "version": "0.0.0",
  "license": "MIT",
  "config": {
    "scripty": {
      "path": ".scripty",
      "parallel": true
    }
  },
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "prepare": "husky install",
    "gen:page": "scripty",
    "typeorm": "ts-node --project tsconfig.base.json -O '{\"module\": \"commonjs\", \"experimentalDecorators\": true}' -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "serve:api": "npx nx serve api",
    "serve:dashboard": "npx nx serve dashboard",
    "sync:coverage": "gsutil -m rsync -r -d -x '.DS_Store' coverage gs://cupola-coverage",
    "sync:create:bucket": "gcloud storage buckets create --project=coverage-367922 gs://`date +'%Y-%m-%d'`-cupola-coverage",
    "sync:copy:bucket": " gsutil -m  cp -r 'gs://cupola-coverage/*' gs://`date +'%Y-%m-%d'`-cupola-coverage",
    "sync:videos": " gsutil -m rsync -r -d -x '.DS_Store' dist/cypress gs://cupola-cypress-videos",
    "gpg": "./csdev.sh local gpg",
    "deployment:email": "node ./tools/scripts/send_email.js "
  },
  "private": true,
